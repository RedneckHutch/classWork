#variables
a=17
s= "hi"

#calc		#all are based off term of a for this example, imagine it is 17 at each line
a +=3      	#a=20 after this is executed
a -=3		#a=14 after this is executed
a *=3		#a=51 after this is executed
a /=3		#a=5.6 after this is executed
a %=3		#a=2 after this is executed
s +=" there"	#s= "hi there" after this is executed
